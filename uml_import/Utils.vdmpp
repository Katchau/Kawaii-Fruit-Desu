class Utils
types
  public string = seq of char;
	public BasketType = <Big> | <Small>;
	public ProdGrams = map Product to nat1
	inv ProdGrams == not exists p1, p2 in set dom ProdGrams & p1 <> p2 and p1.is_equal(p2); -- ver se é preciso isto
functions
	public stringsEquals: string * string -> bool
	stringsEquals(s1,s2) == s1 = s2;
end Utils
