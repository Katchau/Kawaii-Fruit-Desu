class Product
instance variables
  name: Utils`string;

operations
	public Product: Utils`string ==> Product
	Product(n) == (
		name := n;
		return self
	)
	pre len n > 0
	post name = n;
	
  public pure is_equal : Product ==> bool
  	is_equal(prod) == Utils`stringsEquals(name, prod.name)
  	pre prod <> undefined
  	post RESULT = Utils`stringsEquals(name, prod.name);


end Product
