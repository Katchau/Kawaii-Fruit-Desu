  public  getBasket : ()  ==> bool
  getBasket() ==
    is not yet specified;

  public  cancelBasket : ()  ==> ?
  cancelBasket() ==
    is not yet specified;

  public  quitCollective : ()  ==> ?
  quitCollective() ==
    is not yet specified;


end Client
